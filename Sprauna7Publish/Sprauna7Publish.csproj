    <ProjectReference Include="..\Plugins.DataStore.SQL\Plugins.DataStore.SQL.csproj" />
    <ProjectReference Include="..\Plugins.ExpressPay\Plugins.ExpressPay.csproj" />
    <ProjectReference Include="..\Plugins.MailKitSp\Plugins.MailKitSp.csproj" />
    <ProjectReference Include="..\Plugins.MailSp\Plugins.MailSp.csproj" />
    <ProjectReference Include="..\Plugins.ShopByShop.InMemory\Plugins.ShopByShop.InMemory.csproj" />
    <ProjectReference Include="..\Plugins.ShopByShop\Plugins.ShopByShop.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="logger\" />
    <Folder Include="files\" />
    <Folder Include="wwwroot\files\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="BootstrapBlazor.Analyzers" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
